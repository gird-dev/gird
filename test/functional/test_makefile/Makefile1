.gird/tmp/function_dep:
	python -c 'from girdfile import function_dep; function_dep()'

.PHONY: file0
file0: file0__deps
	$(MAKE) --file .gird/tmp/Makefile2 file0

.PHONY: file0__deps
file0__deps:

.PHONY: test
test: test__deps
	$(MAKE) --file .gird/tmp/Makefile2 test

.PHONY: test__deps
test__deps:

.PHONY: package.whl
package.whl: package.whl__deps
	$(MAKE) --file .gird/tmp/Makefile2 package.whl

.PHONY: package.whl__deps
package.whl__deps:

.PHONY: all
all: all__deps
	$(MAKE) --file .gird/tmp/Makefile2 all

.PHONY: all__deps
all__deps: test__deps package.whl__deps

.PHONY: file1
file1: file1__deps
	$(MAKE) --file .gird/tmp/Makefile2 file1

.PHONY: file1__deps
file1__deps: .gird/tmp/function_dep

.PHONY: file2
file2: file2__deps
	$(MAKE) --file .gird/tmp/Makefile2 file2

.PHONY: file2__deps
file2__deps: .gird/tmp/function_dep

.PHONY: file3
file3: file3__deps
	$(MAKE) --file .gird/tmp/Makefile2 file3

.PHONY: file3__deps
file3__deps: file1__deps file2__deps

.PHONY: file4
file4: file4__deps
	$(MAKE) --file .gird/tmp/Makefile2 file4

.PHONY: file4__deps
file4__deps: file3__deps
